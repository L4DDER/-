age=input()
if age <0:
        print("ne pravilnay god rozhdeniy")
elif age <2:
        print("mladenec")
elif age < 4:
        print("malish")
elif age < 13:
        print("rebenok")
elif age < 20:
        print("podrostok")
elif age < 65:
        print("vzrosliu")
else:
        print("pozhiloy chelovek")
